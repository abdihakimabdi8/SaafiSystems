@model List<SaafiSystems.Models.Load>


<h1>@ViewBag.title</h1>
@*@if (Model.Count == 0)
{
    <p>No load entries to remove</p>

}*@

<form asp-controller="Load" asp-action="Remove" method="post">


    @foreach (var entry in Model)
    {
        <input type="checkbox" name="expenseIds" id="@entry.ID" value="@entry.ID" />
        <br />
    }

    <input type="submit" value="Remove Selected Trip(s)" />
</form>