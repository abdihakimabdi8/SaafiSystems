@model List<SaafiSystems.Models.Expense>

<h1>@ViewBag.title</h1>
@if (Model.Count == 0)
{
    <p>No expense entries to remove</p>

}

<form asp-controller="Expense" asp-action="Remove" method="post">


    @foreach (var entry in Model)
    {
        <input type="checkbox" name="expenseIds" id="@entry.ID" value="@entry.ID" />
        <br />
    }

    <input type="submit" value="Remove Selected Expenses(s)" />
</form>